# SUPABASE CONFIGURATION
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# API CONFIGURATION
VITE_API_URL=https://api.spotmap.local
VITE_API_TIMEOUT=30000

# SECURITY CONFIGURATION
VITE_ENABLE_SECURITY_GUARD=true
VITE_ENABLE_FINGERPRINTING=true
VITE_BLOCK_DEVTOOLS=true
VITE_BLOCK_RIGHT_CLICK=true
VITE_BLOCK_SHORTCUTS=true

# BACKEND SECURITY
BACKEND_CSRF_ENABLED=true
BACKEND_RATE_LIMIT=60
BACKEND_IP_BLOCKING=true
BACKEND_LOG_LEVEL=error

# MONITORING & LOGGING
VITE_LOG_ERRORS=true
VITE_SEND_ERROR_REPORTS=true
VITE_ERROR_REPORT_URL=https://api.spotmap.local/api/errors/report

# CACHE CONFIGURATION
VITE_CACHE_ENABLED=true
VITE_CACHE_DURATION=3600000

# DATABASE
DB_HOST=localhost
DB_PORT=5432
DB_NAME=spotmap_prod
DB_USER=spotmap_user
DB_PASSWORD_ENCRYPTED=true

# FEATURE FLAGS
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_BETA_FEATURES=false
VITE_MAINTENANCE_MODE=false

# NODE ENVIRONMENT
NODE_ENV=production
